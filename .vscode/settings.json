    "git.inputValidationLength": 72,
    "[git-commit]": {
        "editor.rulers": [
            50,
            72
        ]
    },
    "[markdown]": {
        "editor.rulers": [80]
    },
    "files.eol": "\n",
    "cSpell.words": [
    ]
}
